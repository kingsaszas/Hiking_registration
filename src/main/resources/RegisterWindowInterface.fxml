<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="420.0" prefWidth="300.0" stylesheets="@RegisterWindowStyle.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="DesktopApplication.RegisterWindowController">
   <children>
      <Pane layoutY="2.0" prefHeight="420.0" prefWidth="300.0">
         <children>
            <Label layoutX="70.0" layoutY="14.0" text="REGISTRATION">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
            <Label layoutX="17.0" layoutY="59.0" text="name" />
            <TextField fx:id="nameField" layoutX="11.0" layoutY="76.0" prefHeight="25.0" prefWidth="277.0" />
            <Label layoutX="17.0" layoutY="111.0" text="surname" />
            <TextField fx:id="SurnameField" layoutX="11.0" layoutY="128.0" prefHeight="25.0" prefWidth="277.0" />
            <Label layoutX="18.0" layoutY="161.0" text="e-mail" />
            <TextField fx:id="emailField" layoutX="12.0" layoutY="178.0" prefHeight="25.0" prefWidth="277.0" />
            <Label layoutX="18.0" layoutY="210.0" text="login" />
            <TextField fx:id="loginField" layoutX="12.0" layoutY="227.0" prefHeight="25.0" prefWidth="277.0" />
            <Label layoutX="17.0" layoutY="258.0" text="password" />
            <TextField fx:id="passField" layoutX="11.0" layoutY="275.0" prefHeight="25.0" prefWidth="277.0" />
            <Label layoutX="18.0" layoutY="306.0" text="repeat password" />
            <TextField fx:id="repeatPassField" layoutX="12.0" layoutY="323.0" prefHeight="25.0" prefWidth="277.0" />
            <Button fx:id="sendBtn" layoutX="230.0" layoutY="373.0" mnemonicParsing="false" onMouseClicked="#sendOnMouseClicked" text="SEND" />
         </children></Pane>
   </children>
</AnchorPane>
